<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
				<Bar>
					<contentRight>
						<Button class="sapUiSmallMarginEnd" id="accept" text="{i18n>accept}"
							type="Accept"  press="handleAcceptPress" />
						<Button class="sapUiSmallMarginEnd" id="reject" text="{i18n>reject}"
							type="Reject" press="handleRejectPress" />
					</contentRight>
				</Bar>

</core:FragmentDefinition>